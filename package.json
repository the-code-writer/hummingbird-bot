{
  "name": "typescript-template",
  "version": "1.0.7",
  "description": "A complete Node.js project template using TypeScript and following general best practices.",
  "keywords": [
    "typescript",
    "template"
  ],
  "author": {
    "name": "The ~Code Writer",
    "url": "https://thecodewriter.io"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/chriswells0/node-typescript-template.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && tsc",
    "clean": "node -e \"var { rmdirSync, existsSync } = require('fs'), path = require('path'); ['./.nyc_output', './coverage', './dist'].forEach(fPath => {if (existsSync(path.join(__dirname, fPath))) rmdirSync(path.join(__dirname, fPath), { recursive: true })}); process.exit(0);\"",
    "clean:all": "npm run clean && (rm -r ./node_modules || true)",
    "cover": "nyc npm run test",
    "cover:check": "nyc --check-coverage --reporter=lcov npm run test",
    "lint": "eslint --ext .ts --fix src/**/*.ts test/**/*.ts",
    "preversion": "npm run build && npm run cover:check",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "serve": "nodemon ./src/index.ts",
    "test": "mocha",
    "prestart": "tsc",
    "upgrade": "npx npm-check -u",
    "dev": "nodemon ./src/app.ts",
    "start": "ts-node src/app.ts"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.5",
    "@types/chai": "^5.0.1",
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "axios": "^1.7.9",
    "chai": "^5.1.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "express": "^4.21.2",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "mocha": "^11.0.1",
    "nodemon": "^3.1.9",
    "npm-check-updates": "^16.14.11",
    "nyc": "^17.1.0",
    "pino-http": "^10.3.0",
    "pnpm": "^9.15.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-template": "file:"
  },
  "dependencies": {
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/umi": "^0.9.2",
    "@metaplex-foundation/umi-bundle-defaults": "^0.9.2",
    "@raydium-io/raydium-sdk-v2": "0.1.99-alpha",
    "@solana/spl-token": "^0.4.9",
    "@solana/spl-token-registry": "^0.2.4574",
    "@solana/web3.js": "^1.98.0",
    "@triton-one/yellowstone-grpc": "^1.2.0",
    "@types/jsonfile": "^6.1.4",
    "bip39": "^3.1.0",
    "bs58": "^6.0.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.7",
    "ioredis": "^5.4.2",
    "jsonfile": "^6.1.0",
    "node-telegram-bot-api": "^0.66.0",
    "sitka": "^1.1.1",
    "spl-token": "link:@solana\\spl-token"
  }
}
